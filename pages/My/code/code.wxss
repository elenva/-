/* pages/My/code/code.wxss */
.wrap {
  background-color: #F2F2F2;
  padding-top: 160rpx;
  height: 100vh;
}
.wrap .inner {
  margin: 0 auto;
  width: 630rpx;
  height: 600rpx;
  position: relative;
}
.wrap .inner > image {
  width: 100%;
  height: 100%;
}
.wrap .inner .content {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
  background-color: transparent;
  padding: 36rpx 0;
  text-align: center;
  width: 100%;
  font-size: 36rpx;
}
.wrap .inner .content > view {
  font-size: 28rpx;
}
.wrap .inner .content > view:nth-child(2) {
  margin-bottom: 108rpx;
}
.wrap .inner .content > image {
  width: 320rpx;
  height: 320rpx;
}
.wrap button {
  margin-top: 60rpx;
  width: 296rpx;
  height: 80rpx;
  line-height: 80rpx;
}